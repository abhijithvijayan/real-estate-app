{
  "extends": [
    "@abhijithvijayan/eslint-config/typescript",
    "@abhijithvijayan/eslint-config/node",
    "@abhijithvijayan/eslint-config/react"
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "rules": {
    "node/no-unsupported-features/es-syntax": ["error", {
      "ignores": ["modules"]
    }]
  }
}