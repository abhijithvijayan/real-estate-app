{
  "name": "real-estate-app",
  "version": "0.1.0",
  "description": "Real estate app using next.js and express",
  "private": true,
  "repository": "https://github.com/abhijithvijayan/real-estate-app.git",
  "author": {
    "name": "abhijithvijayan",
    "email": "email@abhijithvijayan.in",
    "url": "https://abhijithvijayan.in"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev:web": "next dev client/",
    "build:web": "next build client/",
    "dev:server": "nodemon server/server.ts",
    "build": "rimraf production-server && tsc --project tsconfig.json && yarn build:web",
    "start": "cross-env NODE_ENV=production node production-server/server.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f server/ormconfig.ts",
    "migration:create": "yarn typeorm:cli migration:create -n",
    "migration:generate": "yarn typeorm:cli migration:generate -n",
    "migration": "yarn typeorm:cli migration:run -t=false",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --fix --ext .ts,.tsx"
  },
  "dependencies": {
    "advanced-css-reset": "^1.2.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "easy-peasy": "^3.3.1",
    "email-validator": "^2.0.4",
    "emoji-log": "^1.0.2",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.10.0",
    "next": "^9.5.1",
    "nookies": "^2.3.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use-form-state": "^0.13.1",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.1.1",
    "swr": "^0.3.0",
    "tailwindcss": "^1.6.0",
    "twin.macro": "^1.7.0",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@abhijithvijayan/eslint-config": "2.5.5",
    "@abhijithvijayan/eslint-config-airbnb": "^1.0.2",
    "@abhijithvijayan/tsconfig": "^1.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/jwt-decode": "^2.2.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
